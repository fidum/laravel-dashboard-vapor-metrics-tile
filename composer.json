{
    "name": "fidum/laravel-dashboard-vapor-metrics-tile",
    "description": "Vapor environment, cache and database metrics tiles for laravel dashboard",
    "keywords": [
        "fidum",
        "laravel-dashboard",
        "laravel-dashboard-vapor-metrics-tile",
        "vapor"
    ],
    "homepage": "https://github.com/fidum/laravel-dashboard-vapor-metrics-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com",
            "homepage": "https://danielmason.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^1.0",
        "laravel/vapor-cli": "^1.0",
        "illuminate/support": "^7.0",
        "illuminate/console": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "nunomaduro/laravel-mojito": "^0.2.4",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidum\\VaporMetricsTile\\VaporMetricsTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
