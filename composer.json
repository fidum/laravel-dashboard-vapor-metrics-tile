{
    "name": "fidum/laravel-dashboard-vapor-metrics-tile",
    "description": "Vapor environment, cache and database metrics tiles for laravel dashboard",
    "keywords": [
        "fidum",
        "laravel-dashboard",
        "laravel-dashboard-vapor-metrics-tile",
        "vapor"
    ],
    "homepage": "https://github.com/fidum/laravel-dashboard-vapor-metrics-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com",
            "homepage": "https://fidum.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "fidum/laravel-dashboard-chart-tile": "^6.0",
        "laravel/vapor-cli": "^1.60",
        "spatie/laravel-dashboard": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidum\\VaporMetricsTile\\VaporMetricsTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
