{
    "name": "fidum/laravel-dashboard-vapor-metrics-tile",
    "description": "Vapor environment, cache and database metrics tiles for laravel dashboard",
    "keywords": [
        "fidum",
        "laravel-dashboard",
        "laravel-dashboard-vapor-metrics-tile",
        "vapor"
    ],
    "homepage": "https://github.com/fidum/laravel-dashboard-vapor-metrics-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com",
            "homepage": "https://fidum.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "fidum/laravel-dashboard-chart-tile": "^4.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0",
        "laravel/vapor-cli": "^1.10",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "nunomaduro/laravel-mojito": "^0.2.6",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidum\\VaporMetricsTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidum\\VaporMetricsTile\\VaporMetricsTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
